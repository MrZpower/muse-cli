{
  "name": "muse-cli",
  "version": "1.1.5",
  "description": "MuseCLI generates a discord.js bot with music features for you. You can use the generate command to automatically generate boilerplate for commands and events so you don't have to keep re-writing the same code every time you create a new component.",
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "index.js",
  "bin": {
    "muse": "index.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/@spimy"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Spimy/muse-cli.git"
  },
  "author": "Spimy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Spimy/muse-cli/issues"
  },
  "homepage": "https://github.com/Spimy/muse-cli#readme",
  "dependencies": {
    "arg": "^4.1.3",
    "chalk": "^4.1.0",
    "esm": "^3.2.25",
    "execa": "^4.0.2",
    "inquirer": "^7.2.0",
    "listr": "^0.14.3",
    "ncp": "^2.0.0",
    "pkg-install": "^1.0.0"
  },
  "keywords": [
    "nodejs",
    "discord-music-bot",
    "music-bot",
    "music-functions",
    "music-player",
    "discord.js",
